char* convert(char* s, int numRows) {
    char *res = malloc(sizeof(char) * strlen(s));
    if (res == NULL)
        return (NULL);

    int i = 0;
    int cut = 0;
    int count = 1;
    while (*s)
    {
        if (count % 2 == 0)
            res[i] = *(s + cut);
        else
            res[i] = *s;
        s++, i++, count++;
        cut += numRows;
    }
}